<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <!-- SeekBar -->
    <declare-styleable name="CustomSeekBar" >
        <attr name="corner_radius" format="dimension" />
        <attr name="min_value" format="integer" />
        <attr name="max_value" format="integer"/>
        <attr name="min_start_value" format="integer" />
        <attr name="max_start_value" format="integer"/>
        <attr name="steps" format="integer"/>
        <attr name="gap" format="dimension"/>
        <attr name="fix_gap" format="dimension"/>
        <attr name="bar_height" format="dimension" />
        <attr name="bar_color_mode" format="reference"/>
        <attr name="bar_color" format="color"/>
        <attr name="bar_gradient_start" format="reference"/>
        <attr name="bar_gradient_end" format="reference"/>
        <attr name="bar_highlight_color_mode" format="reference"/>
        <attr name="bar_highlight_color" format="color" />
        <attr name="bar_highlight_gradient_start" format="reference"/>
        <attr name="bar_highlight_gradient_end" format="reference"/>
        <attr name="thumb_diameter" format="dimension"/>
        <attr name="position" format="integer"/>
        <attr name="data_type" format="integer"/>
        <attr name="thumb_color" format="color" />
        <attr name="thumb_color_pressed" format="color" />
        <attr name="thumb_image" format="reference" />
        <attr name="thumb_image_pressed" format="reference" />
        <attr name="seek_bar_touch_enabled" format="boolean" />
    </declare-styleable>
</resources>
